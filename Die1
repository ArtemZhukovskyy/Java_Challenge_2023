import java.lang.Math;
public class Die
{
    int roll() {
        int max = 6;
        int min = 1;
        int range = max - min + 1;
        int roll = (int)(Math.random() * range) + min;
        System.out.println("Rolled " + roll);
        return roll;
    }

    public static void main(String[] args){
        Die die = new Die();
        for (int i=0; i<20; i++) {
            die.roll();
        }
    }
}
